{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "RDI $US Values",
  "data": {"url": "assets/charts/data/df_data.csv"},
  "mark": {
    "type": "line",
    "point": true,
    "tooltip": true
  },
  "encoding": {
    "x": {
      "timeUnit": "year",
      "field": "utcyymm",
      "axis": {"title": "Year", "format": "%b %y"}
    },
    "y": {"aggregate": "mean", "field": "usvalue", "type": "quantitative"}
  }
}
